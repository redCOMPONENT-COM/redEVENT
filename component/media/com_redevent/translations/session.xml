<?xml version="1.0"?>
<contentelement type="contentelement">
	<name>redEVENT sessions</name>
	<author>redCOMPONENT</author>
	<copyright>Copyright (C) 2008 - 2016 redCOMPONENT.com. All rights reserved.</copyright>
	<version>3.2.1</version>
	<description>Sessions Table Definition for the redEVENT extension</description>
	<reference>
		<table name="redevent_event_venue_xref">
			<field name="id" type="referenceid" translate="0" alwaysFallback="false" filter="RAW" description="">ID</field>
			<field name="title" type="text" translate="1" alwaysFallback="false" filter="RAW" description="">Name</field>
			<field name="alias" type="text" translate="1" alwaysFallback="false" filter="RAW" description="">Alias</field>
			<field name="external_registration_url" type="text" translate="1" alwaysFallback="false" filter="RAW" description="">External registration url</field>
			<field name="note" type="text" translate="1" alwaysFallback="false" filter="RAW" description="">Note</field>
			<field name="details" type="htmltext" translate="1" alwaysFallback="false" filter="RAW" description="">Description</field>
			<field name="icaldetails" type="htmltext" translate="1" alwaysFallback="false" filter="RAW" description="">Description for ical</field>
			<field name="icalvenue" type="htmltext" translate="1" alwaysFallback="false" filter="RAW" description="">Venue title for ical</field>
		</table>

		<component>
			<editForm admin="true" option="com_redevent" view="session" layout="edit" identifier="id" showbutton="true" htmlposition=".btn-toolbar:first .btn-group:first" checkoriginalid="false"/>
		</component>
	</reference>
</contentelement>
